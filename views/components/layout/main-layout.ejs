            <div class="col-md-10">
              <div class="main">
                <div class="breadcrumb d-flex align-items-center gap-2">
                  <span class="fs-12"> Cars </span>
                  <img src="/images/Vector_right.png" alt="" />
                  <span class="active fs-12">List Car</span>
                  <span class="fs-12"
                    ><a href="/" class="text-decoration-none text-black"
                      >List Car</a
                    >
                  </span>
                  <img src="/images/Vector_right.png" alt="" />
                  <span class="active fs-12"> yy</span>
                </div>
                <!-- Main Content -->
                <div class="row align-items-center mt-3">
                  <div class="col-md-6">
                    <h1 class="main-title fs-20 m-0 fw-bold">List Car</h1>
                  </div>
                  <div class="col-md-6 d-flex justify-content-end">
                    <a
                      class="btn btn-primary text-white fs-14 fw-bold"
                      href="/cars/create"
                    >
                      <img class="me-2" src="/images/fi_plus.png" alt="plus" />
                      Add New Car
                    </a>
                  </div>
                </div>
                <div class="row">
                  <div class="filter d-flex gap-3 my-4">
                    <button class="btn btn-outline-primary active">All</button>
                    <button class="btn btn-outline-secondary">Small</button>
                    <button class="btn btn-outline-secondary">Medium</button>
                    <button class="btn btn-outline-secondary">Large</button>
                  </div>
                </div>
                <div class="cars-container">
                  <div class="row">
                    <!-- Card -->
                    <div class="col-md-4 mt-4">
                      <div class="card">
                        <img
                          src="/images/image_1.png"
                          class="card-img"
                          alt="Car Image"
                        />
                        <div class="card-body p-0">
                          <h5 class="card-title mt-3 mb-1 fs-14">
                            MobilMobilan
                          </h5>
                          <p class="card-harga mb-1 fs-16">Rp 200.000/ hari</p>
                          <div class="card-info mt-3">
                            <div class="info">
                              <p class="clock d-flex fs-14">
                                <img
                                  class="me-2"
                                  src="/images/fi_clock.png"
                                  alt="clock icon"
                                />
                                <span class="ml-2">Rabu</span>
                              </p>
                            </div>
                          </div>
                        </div>
                        <div class="row mt-4">
                          <div class="col-md-6">
                            <a
                              class="delete_btn font-weight-bold btn btn-outline-danger d-flex align-items-center justify-content-center"
                              href="#"
                            >
                              <img
                                class="me-2"
                                src="/images/fi_trash-2.png"
                                alt=""
                              />
                              Delete</a
                            >
                          </div>
                          <div class="col-md-6">
