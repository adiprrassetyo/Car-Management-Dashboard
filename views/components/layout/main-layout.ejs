    <!-- Bootstrap CSS -->
    <link rel="shortcut icon" href="./images/icon.png" type="image" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
