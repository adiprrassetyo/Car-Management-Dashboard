<% if(typeof errors != 'undefined'){%>
<% errors.forEach(function (error) { %>
<div class="alert alert-primary alert-dismissible fade show" role="alert">
  <strong><%= error.msg %></strong>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}) ;%> <%}%>